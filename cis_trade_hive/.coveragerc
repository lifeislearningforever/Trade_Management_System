[run]
source = .
omit =
    */tests/*
    */test_*.py
    */migrations/*
    */__pycache__/*
    */venv/*
    */.venv/*
    manage.py
    */wsgi.py
    */asgi.py
    */settings.py
    */apps.py
    config/*

[report]
precision = 2
show_missing = True
skip_covered = False
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
